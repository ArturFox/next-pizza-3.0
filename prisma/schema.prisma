generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  FirstName  String
  SecondName String
  email      String
  password   String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @default(now())
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String
  imageUrl  String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
